#include "stm32f1xx_hal.h"

void SystemClockConfig(void);
void UART2_Init(void);

UART_HandleTypeDef huart2;

int main(void){
	HAL_Init();
	SystemClockConfig();
	UART2_Init();

	return 0;
}

void SystemClockConfig(void){
	// use internal rc oscillator
}

void UART2_Init(void){
	huart2.Instance = USART2;	// Link to base address of the peripheral
}
